{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport Navbar from './Navbar.vue';\nimport db from '../firebaseDb';\nimport { collection, onSnapshot, doc, deleteDoc, query, where } from 'firebase/firestore';\nconst subsCollectionRef = collection(db, 'subscriptions'); // Database collection reference\n\nexport default {\n  name: \"List-vue\",\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      Subscriptions: [],\n      user: '',\n      subscriptionImages: {\n        'Amazon Prime': require('../assets/amazon.png'),\n        'Disney+': require('../assets/disney.png'),\n        'HBO': require('../assets/hbo.png'),\n        'hulu': require('../assets/hulu.png'),\n        'Default': require('../assets/default.png')\n      }\n    };\n  },\n  async created() {\n    this.user = JSON.parse(localStorage.getItem(\"log_user\"));\n    const q = query(subsCollectionRef, where(\"username\", \"==\", this.user));\n    const dbquery = onSnapshot(q, snapshot => {\n      this.Subscriptions = [];\n      snapshot.forEach(doc => {\n        if (doc.data().name) {\n          this.Subscriptions.push({\n            key: doc.id,\n            name: doc.data().name,\n            description: doc.data().description,\n            amount: doc.data().amount,\n            frequency: doc.data().frequency,\n            date: doc.data().date,\n            username: doc.data().username\n          });\n        }\n      });\n    });\n  },\n  mounted() {\n    this.user = JSON.parse(localStorage.getItem(\"log_user\"));\n  },\n  methods: {\n    deleteSubscription(id) {\n      if (window.confirm('Do you really want to delete?')) {\n        deleteDoc(doc(subsCollectionRef, id)).then(() => {}).catch(error => {\n          console.error(error);\n        });\n      }\n    },\n    createSubscription() {\n      this.$router.push('/create');\n    },\n    getSubscriptionImage(subscriptionName) {\n      return this.subscriptionImages[subscriptionName] || this.subscriptionImages['Default'];\n    }\n  }\n};","map":{"version":3,"names":["Navbar","db","collection","onSnapshot","doc","deleteDoc","query","where","subsCollectionRef","name","components","data","Subscriptions","user","subscriptionImages","require","created","JSON","parse","localStorage","getItem","q","dbquery","snapshot","forEach","push","key","id","description","amount","frequency","date","username","mounted","methods","deleteSubscription","window","confirm","then","catch","error","console","createSubscription","$router","getSubscriptionImage","subscriptionName"],"sources":["/Users/leothecub/PycharmProjects/pythonActivities/subscriptions/src/components/SubscriptionsList.vue"],"sourcesContent":["<template>\n    <div class=\"app\">\n        <Navbar />\n        <div class=\"subscription-list\">\n            <div class=\"row justify-content-center\">\n                <div class=\"col-md-10\">\n                    <div class=\"d-flex justify-content-between align-items-center mb-4\">\n                        <h2 class=\"section-title\">Your Active Subscriptions</h2>\n                        <button @click=\"createSubscription\" class=\"btn btn-primary btn-lg\">\n                            <i class=\"bi bi-plus-circle-fill me-2\"></i> Add New\n                        </button>\n                    </div>\n                    <div class=\"row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4\">\n                        <div v-for=\"subscription in Subscriptions\" :key=\"subscription.key\" class=\"col\">\n                            <div class=\"card h-100 shadow-sm\">\n                                <div class=\"img-container\">\n                                    <img :src=\"getSubscriptionImage(subscription.name)\" class=\"card-img-top square-image\" alt=\"Subscription Image\">\n                                </div>\n                                <div class=\"card-body d-flex flex-column justify-content-between\">\n                                    <div>\n                                        <h5 class=\"card-title mb-2\">{{ subscription.name }}</h5>\n                                        <p class=\"card-text text-muted d-none d-sm-block mb-2\">{{ subscription.description }}</p>\n                                        <p class=\"card-text mb-1\"><strong class=\"me-1\">Amount:</strong> ${{ subscription.amount }}</p>\n                                        <p class=\"card-text mb-1\"><small class=\"text-muted me-1\">Frequency:</small> {{ subscription.frequency }}</p>\n                                        <p class=\"card-text\"><small class=\"text-muted me-1\">Joined:</small> {{ subscription.date }}</p>\n                                    </div>\n                                    <div class=\"mt-3\">\n                                        <div class=\"d-grid gap-2\">\n                                            <router-link :to=\"{ name: 'edit', params: { id: subscription.key } }\"\n                                                class=\"btn btn-outline-primary btn-sm\">\n                                                <i class=\"bi bi-pencil-square me-2\"></i> Edit\n                                            </router-link>\n                                            <button @click.prevent=\"deleteSubscription(subscription.key)\"\n                                                class=\"btn btn-outline-danger btn-sm\">\n                                                <i class=\"bi bi-trash-fill me-2\"></i> Delete\n                                            </button>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div v-if=\"Subscriptions.length === 0\" class=\"text-center mt-5\">\n                        <p class=\"lead\">You don't have any active subscriptions yet.</p>\n                        <button @click=\"createSubscription\" class=\"btn btn-primary btn-lg\">\n                            <i class=\"bi bi-plus-circle-fill me-2\"></i> Add Your First Subscription\n                        </button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport Navbar from './Navbar.vue'\nimport db from '../firebaseDb'\nimport { collection, onSnapshot, doc, deleteDoc, query, where } from 'firebase/firestore'\nconst subsCollectionRef = collection(db, 'subscriptions') // Database collection reference\n\nexport default {\n    name: \"List-vue\",\n    components: {\n        Navbar\n    },\n    data() {\n        return {\n            Subscriptions: [],\n            user: '',\n            subscriptionImages: {\n                'Amazon Prime': require('../assets/amazon.png'),\n                'Disney+': require('../assets/disney.png'),\n                'HBO': require('../assets/hbo.png'),\n                'hulu': require('../assets/hulu.png'),\n                'Default': require('../assets/default.png'),\n            }\n        }\n    },\n    async created() {\n        this.user = JSON.parse(localStorage.getItem(\"log_user\"));\n        const q = query(subsCollectionRef, where(\"username\", \"==\", this.user));\n        const dbquery = onSnapshot(q, (snapshot) => {\n            this.Subscriptions = []\n            snapshot.forEach((doc) => {\n                if (doc.data().name) {\n                    this.Subscriptions.push({\n                        key: doc.id,\n                        name: doc.data().name,\n                        description: doc.data().description,\n                        amount: doc.data().amount,\n                        frequency: doc.data().frequency,\n                        date: doc.data().date,\n                        username: doc.data().username,\n                    })\n                }\n            })\n        })\n    },\n    mounted() {\n        this.user = JSON.parse(localStorage.getItem(\"log_user\"));\n    },\n    methods: {\n        deleteSubscription(id) {\n            if (window.confirm('Do you really want to delete?')) {\n                deleteDoc(doc(subsCollectionRef, id)).then(() => {\n                })\n                    .catch((error) => {\n                        console.error(error)\n                    })\n            }\n        },\n        createSubscription() {\n            this.$router.push('/create')\n        },\n        getSubscriptionImage(subscriptionName) {\n            return this.subscriptionImages[subscriptionName] || this.subscriptionImages['Default'];\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.subscription-list {\n    padding: 30px 20px;\n}\n\n.section-title {\n    font-size: 2rem;\n    color: #343a40;\n    margin-bottom: 0;\n}\n\n.card {\n    border: none;\n    border-radius: 12px;\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.08);\n    transition: transform 0.2s ease-in-out;\n\n    &:hover {\n        transform: translateY(-4px);\n    }\n}\n\n.img-container {\n    width: 100%;\n    overflow: hidden;\n    border-top-left-radius: 12px;\n    border-top-right-radius: 12px;\n}\n\n.square-image {\n    width: 100%;\n    height: auto;\n    aspect-ratio: 1 / 1;\n    object-fit: cover;\n    border-top-left-radius: 12px;\n    border-top-right-radius: 12px;\n}\n\n.card-body {\n    padding: 1.5rem;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n}\n\n.card-title {\n    font-size: 1.1rem;\n    font-weight: 600;\n    color: #212529;\n    margin-bottom: 0.5rem;\n}\n\n.card-text {\n    font-size: 0.9rem;\n    color: #495057;\n    margin-bottom: 0.25rem;\n\n    strong {\n        font-weight: 500;\n    }\n}\n\n.btn-outline-primary,\n.btn-outline-danger,\n.btn-primary {\n    font-size: 0.85rem;\n    padding: 0.6rem 1rem;\n    border-radius: 8px;\n    transition: all 0.15s ease-in-out;\n}\n\n.btn-primary {\n    background-color: #007bff;\n    border-color: #007bff;\n\n    &:hover {\n        background-color: #0056b3;\n        border-color: #0056b3;\n    }\n}\n\n.btn-outline-primary {\n    color: #007bff;\n    border-color: #007bff;\n\n    &:hover {\n        background-color: #007bff;\n        color: #fff;\n        border-color: #007bff;\n    }\n}\n\n.btn-outline-danger {\n    color: #dc3545;\n    border-color: #dc3545;\n\n    &:hover {\n        background-color: #dc3545;\n        color: #fff;\n        border-color: #dc3545;\n    }\n}\n\n.btn-lg {\n    font-size: 1rem;\n    padding: 0.75rem 1.5rem;\n    border-radius: 10px;\n}\n</style>"],"mappings":";;;AAuDA,OAAOA,MAAK,MAAO,cAAa;AAChC,OAAOC,EAAC,MAAO,eAAc;AAC7B,SAASC,UAAU,EAAEC,UAAU,EAAEC,GAAG,EAAEC,SAAS,EAAEC,KAAK,EAAEC,KAAI,QAAS,oBAAmB;AACxF,MAAMC,iBAAgB,GAAIN,UAAU,CAACD,EAAE,EAAE,eAAe,GAAE;;AAE1D,eAAe;EACXQ,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACRV;EACJ,CAAC;EACDW,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,aAAa,EAAE,EAAE;MACjBC,IAAI,EAAE,EAAE;MACRC,kBAAkB,EAAE;QAChB,cAAc,EAAEC,OAAO,CAAC,sBAAsB,CAAC;QAC/C,SAAS,EAAEA,OAAO,CAAC,sBAAsB,CAAC;QAC1C,KAAK,EAAEA,OAAO,CAAC,mBAAmB,CAAC;QACnC,MAAM,EAAEA,OAAO,CAAC,oBAAoB,CAAC;QACrC,SAAS,EAAEA,OAAO,CAAC,uBAAuB;MAC9C;IACJ;EACJ,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACZ,IAAI,CAACH,IAAG,GAAII,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,CAAC;IACxD,MAAMC,CAAA,GAAIf,KAAK,CAACE,iBAAiB,EAAED,KAAK,CAAC,UAAU,EAAE,IAAI,EAAE,IAAI,CAACM,IAAI,CAAC,CAAC;IACtE,MAAMS,OAAM,GAAInB,UAAU,CAACkB,CAAC,EAAGE,QAAQ,IAAK;MACxC,IAAI,CAACX,aAAY,GAAI,EAAC;MACtBW,QAAQ,CAACC,OAAO,CAAEpB,GAAG,IAAK;QACtB,IAAIA,GAAG,CAACO,IAAI,CAAC,CAAC,CAACF,IAAI,EAAE;UACjB,IAAI,CAACG,aAAa,CAACa,IAAI,CAAC;YACpBC,GAAG,EAAEtB,GAAG,CAACuB,EAAE;YACXlB,IAAI,EAAEL,GAAG,CAACO,IAAI,CAAC,CAAC,CAACF,IAAI;YACrBmB,WAAW,EAAExB,GAAG,CAACO,IAAI,CAAC,CAAC,CAACiB,WAAW;YACnCC,MAAM,EAAEzB,GAAG,CAACO,IAAI,CAAC,CAAC,CAACkB,MAAM;YACzBC,SAAS,EAAE1B,GAAG,CAACO,IAAI,CAAC,CAAC,CAACmB,SAAS;YAC/BC,IAAI,EAAE3B,GAAG,CAACO,IAAI,CAAC,CAAC,CAACoB,IAAI